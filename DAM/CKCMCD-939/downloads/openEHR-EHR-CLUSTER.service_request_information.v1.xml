  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Service request information</items>
        <items id="description">A generic pattern for holding additional questions/answer pairs that relate to a requested service. Information recorded in this pattern is textual only, capturing elements of information that might "travel with" the service request, being pulled from already documented data in the patient record, rather than being entered directly by the ordering clinician.</items>
      </items>
      <items code="at0002">
        <items id="text">Answer</items>
        <items id="description">Intended to communicate available responses to a question or a field needed to complete a service request.</items>
      </items>
    </term_definitions>
  </ontology>
